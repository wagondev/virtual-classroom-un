<div class="container-fluid">

		<div class="text-right">
			<%- if controller_name != 'sessions' %>
			 <div >
			 	 <%= link_to "Inicia Sesión", new_session_path(resource_name), class:"text-warning" %><br />
			 </div>
			<% end -%>
			
			<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
			  <div>
			  	<%= link_to "Crea una cuenta", new_registration_path(resource_name), class:"text-success" %>
			  	<br>
			  </div>
			<% end -%>
			
			<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
			  <div>
			  	<%= link_to "Olvidaste tu contraseña?", new_password_path(resource_name) %><br />
			  </div>
			<% end -%>
			
			<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
			  <div>
			  	<%= link_to "¿No recibiste instrucciones de confirmación?", new_confirmation_path(resource_name) %><br />
			  </div>
			<% end -%>
			
			<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
			  <div>
			  	<%= link_to "¿No recibiste las instrucciones de desbloqueo?", new_unlock_path(resource_name) %><br />
			  </div>
			<% end -%>
			
			<%- if devise_mapping.omniauthable? %>
			  <%- resource_class.omniauth_providers.each do |provider| %>
			    <div>
			    	<%= link_to "Inicia Sesión con #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) %><br />
			    </div>
			  <% end -%>
			<% end -%></div>
</div>